	</content-types>
	<content-generator
		id="localizedmondrian"
		title="Localizedmondrian"
		type="localizedmondrian"
		class="pt.webdetails.cpk.CpkContentGenerator"/>
	<!-- Menu entry -->
	<menu-items>
		<!--<menu-item id="localizedmondrian_main" anchor="tools-submenu" label="Localizedmondrian" command="content/localizedmondrian/" type="MENU_ITEM" how="LAST_CHILD"/>-->
