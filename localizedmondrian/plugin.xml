		<overlay id="startup.localizedmondrian" resourcebundle="content/localizedmondrian/resources/lang/messages">
			<menubar id="toolsmenu">
	        	<menuitem id="localizedmondrian" label="${Launcher.localizedmondrian}" command="mantleXulHandler.openUrl('${Launcher.localizedmondrian}','${Launcher.localizedmondrian_TOOLTIP}','plugin/localizedmondrian/api/default')"/>
